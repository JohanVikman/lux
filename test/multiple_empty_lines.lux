[doc Test multiple empty lines in regexp]

[shell empty]
    """!
    cat > multiple_empty_lines.txt
    abc
    def



    gih
    ${_CTRL_D_}
    """

    ?SH-PROMPT:
    !cat multiple_empty_lines.txt
    """?
    abc
    def



    gih
    SH-PROMPT:
    """

    !myvar="
    !line1
    !line2
    !line3
    !"
    ?SH-PROMPT:

    !echo "$$myvar"
    ?\R\R(line1\Rline2\Rline3\R)\R
    [global result=$1]

    !echo "abc" > foo
    !echo >> foo
    !echo >> foo
    !echo >> foo
    !echo "def" >> foo
    ?SH-PROMPT:

    !cat foo
    """???
    abc



    def
    """

    !cat foo
    """?
    abc



    def
    """

    !cat foo
    """?
    abc
    \R{2}
    def
    """

    !cat foo
    ?abc\R{4}def

[cleanup]
    !rm -f multiple_empty_lines.txt foo
    ?SH-PROMPT:
